{
  "name": "react-in-time",
  "version": "1.1.3",
  "description": "A keyboard friendly react component for capturing time",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "prepublish": "npm run build",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo --port 1111",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "15.x",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "keycode": "^2.1.9",
    "nwb": "0.18.x",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "simulant": "^0.2.2",
    "sinon": "^2.4.1"
  },
  "author": "Alan Clarke <hi@alz.io> (alz.io)",
  "homepage": "https://github.com/alanclarke/react-in-time#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanclarke/react-in-time.git"
  },
  "keywords": [
    "react-component"
  ],
  "standard": {
    "ignore": [
      "/lib/*",
      "/es/*"
    ],
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/alanclarke/react-in-time/issues"
  }
}
